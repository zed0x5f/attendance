{
  "rules": {
    "members": {
      ".read": "auth.uid != null",
      ".write": "auth.uid != null && auth.token.role === 'admin'"
    },
    "checkin": {
      ".read": "auth.uid != null && (auth.token.role == 'admin' || auth.token.role == 'manager')",
      ".write": "auth.uid != null && (auth.token.role == 'admin' || auth.token.role == 'manager')"
    },
    "reservation":{
      ".read":"true",
      ".write":"true"
    }
  }
}
